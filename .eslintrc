  
{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens":"off",
    "comma-dangle":"off",
    "linebreak-style": "off",
    "require-jsdoc":"off",
    "operator-linebreak":"off",
    "max-len":"off",
    "object-curly-spacing": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}